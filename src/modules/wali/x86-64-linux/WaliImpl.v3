component WaliImpl {
	def syscall(num: int, args: Range<Value>) -> HostResult.Value1 {
		var r = Linux.syscall(num, args);
		return HostResult.Value1(Values.box_l(r.0));
	}
	def passthrough(num: int, args: Range<Value>) -> HostResult {
		return syscall(num, args);
	}
	def close(num: int, args: Range<Value>) -> HostResult {
		var r = syscall(num, args);
		// fdmap.free(fd);
		return r;
	}
}
